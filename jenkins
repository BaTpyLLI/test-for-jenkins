pipeline {
    agent any

	stages{
		stage('Check update text.txt') {
          	  when { changeset "**/test.txt" }
		steps{
			sh 'cat test.txt'
            }
				}
 
	stage('SQL') {
            steps {
                sh 'mysql --user rfamro --host mysql-rfam-public.ebi.ac.uk --port 4497 --database Rfam < test.txt'
				}
			}
			}
			}
