pipeline {
    agent any

	stages{
		stage('Check update Jenkins') {
          	  when { changeset "**/jenkins" }
		steps{
			echo 'File update'
            }
				}
 
	stage('Hello') {
            steps {
                sh 'mysql --user rfamro --host mysql-rfam-public.ebi.ac.uk --port 4497 --database Rfam < test.txt'
				}
			}
			}
			}
